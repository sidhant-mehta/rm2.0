<h1>Listing teams</h1>

<table>
  <tr>
    <th>Fname</th>
    <th>Lname</th>
    <th>Bio</th>
    <th>Position</th>
    <th>Image</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.fname %></td>
    <td><%= team.lname %></td>
    <td><%= team.bio %></td>
    <td><%= team.position %></td>
    <td><%= team.image %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
